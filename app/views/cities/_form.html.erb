<%= form_for @city, html: { multipart: true } do |f| %>
<%= render 'layouts/error_messages', target: @city %>
    <div class="form-group">
        <%= f.label :image %>
        <%= f.file_field :image, class: "form-control" %>
    </div>
    <div class="form-group">
        <%= f.label :name %>
        <%= f.text_field :name, class: "form-control" %>
    </div>
    <div class="form-group">
        <%= f.label :state %>
        <%= f.select :state, options_for_select($states), { include_blank: "" }, class: "form-control" %>
    </div> 
    <%= f.submit class: "btn btn-primary"%>
<% end %>