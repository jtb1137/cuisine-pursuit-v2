<%= form_for @city do |f| %>
    <div class="form-group">
        <%= f.label :name %>
        <%= f.text_field :name, class: "form-control" %>
    </div>
    <div class="form-group">
        <%= f.label :state %>
        <%= f.text_field :state, class: "form-control" %>
    </div> 
    <%= f.submit class: "btn btn-primary"%>
<% end %>